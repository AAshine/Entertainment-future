// 标题 TitlePublic CSS
.TitlePublic 
    text-align: center
    padding 57px 0 40px 0
    color black
.addStrip
    display: flex
    width: 1200px
    height 92px
    justify-content: center
    align-items: center
    background #493c98
    padding 30px 0 30px 0
    color #fefeff
    font-size: 2.1rem
    text-align: center
    margin 20px auto
    & .addBtn
        border 1px solid #fefeff
        padding: 12px 15px 11px 15px
        font-weight: 100
        border-radius: 10px
        margin-left 20px
        width: 145px
        font-size: 20px
        font-weight: 500
        height: 50px
    & .addtext
        margin-left 30px
        font-size: 30px
.iconZero 
    border 2px solid #ffffff
    padding 5px
    border-radius: 50%
    width: 60px
    height 60px
